#include<bits/stdc++.h>
int lisInCircularManner(int n, vector<int> &arr) {
    
    vector<int>sorted;

    for(int i=0;i<n;i++){
        auto it=lower_bound(sorted.begin(),sorted.end(),arr[i]);
    
        if(it==sorted.end()){
            sorted.push_back(arr[i]);
        }
        else{
            *it=arr[i];
        }
        
    }
    return sorted.size();
}
